#container{
  position: absolute;
  z-index: 2;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  min-height: 400px;
  min-width: 800px;
}



#container:before{
  content: "";
  position: absolute;
  z-index: 1;
  top: 0;
  right: 0;     
  left: 0;
  height: 175px;
  pointer-events: none;

  background: -moz-linear-gradient(top, rgba(0,0,0,0.7) 0%, rgba(0,0,0,0) 71%, rgba(0,0,0,0) 100%); /* FF3.6+ */
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(0,0,0,1)), color-stop(71%,rgba(0,0,0,0)), color-stop(100%,rgba(0,0,0,0))); /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(top, rgba(0,0,0,0.7) 0%,rgba(0,0,0,0) 71%,rgba(0,0,0,0) 100%); /* Chrome10+,Safari5.1+ */
  background: -o-linear-gradient(top, rgba(0,0,0,0.7) 0%,rgba(0,0,0,0) 71%,rgba(0,0,0,0) 100%); /* Opera 11.10+ */
  background: -ms-linear-gradient(top, rgba(0,0,0,0.7) 0%,rgba(0,0,0,0) 71%,rgba(0,0,0,0) 100%); /* IE10+ */
  background: linear-gradient(top, rgba(0,0,0,0.7) 0%,rgba(0,0,0,0) 71%,rgba(0,0,0,0) 100%); /* W3C */
}


#container:after{
  content: "";
  position: absolute;
  z-index: 1;
  bottom: 0;
  right: 0;     
  left: 0;
  height: 500px;
  pointer-events: none;

  background: -moz-linear-gradient(top, rgba(0,0,0,0) 11%, rgba(0,0,0,0.04) 15%, rgba(0,0,0,0.75) 100%); /* FF3.6+ */
  background: -webkit-gradient(linear, left top, left bottom, color-stop(11%,rgba(0,0,0,0)), color-stop(15%,rgba(0,0,0,0.04)), color-stop(100%,rgba(0,0,0,0.75))); /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(top, rgba(0,0,0,0) 11%,rgba(0,0,0,0.04) 15%,rgba(0,0,0,0.75) 100%); /* Chrome10+,Safari5.1+ */
  background: -o-linear-gradient(top, rgba(0,0,0,0) 11%,rgba(0,0,0,0.04) 15%,rgba(0,0,0,0.75) 100%); /* Opera 11.10+ */
  background: -ms-linear-gradient(top, rgba(0,0,0,0) 11%,rgba(0,0,0,0.04) 15%,rgba(0,0,0,0.75) 100%); /* IE10+ */
  background: linear-gradient(top, rgba(0,0,0,0) 11%,rgba(0,0,0,0.04) 15%,rgba(0,0,0,0.75) 100%); /* W3C */
}


#container > header {
  position: absolute;
  z-index: 3;
  width: 600px;
  top: 50%;
  left: 50%;
  margin-left: -300px;
  margin-top: -200px;
  text-align: center;
}

      
#container > header > h1.logo{
  font-family: "Helvetica Neue", Arial, Helvetica, sans-serif;
  font-size: 58px;
  font-weight: 300;
  letter-spacing: 4px;
  text-align: center;
  width: 100%;  
  margin: 15px 0;
}


  
#container > header > h1.logo .bracket{
  color: #888888;
}


#container > header > h1.logo .giga{
  color: #efefef;
}


#container > header > h1.logo .fied{
  color: #febe23;
}


      
#container > header > nav > ul{
  margin: 0 auto;
  padding: 0px;
  list-style: none;
  display: inline;
}

  
#container > header > nav > ul li{
  margin: 0px 0px;
  color: #b9b9b9;
  display: inline-block;
  text-transform: uppercase;
  font-size: 12px;
}
  
#container > header > nav > ul li a{
  padding: 3px 10px;

 -webkit-transition: all 0.25s ease-out;
 -moz-transition: all 0.25s ease-out;
 -o-transition: all 0.25s ease-out;
 transition: all 0.25s ease-out;
}

  
#container > header > nav > ul li > a:link, 
#container > header > nav > ul li > a:visited{
  color: #b9b9b9;
  text-decoration: none;
}

  
#container > header > nav > ul li > a:active, 
#container > header > nav > ul li > a:hover, 
#container > header > nav > ul li > a.active{
  color: #efefef;
  text-decoration: none;
  background: rgba(0,0,0,0.25);
}


#container > header > nav > ul li:after{
  content: "";
  display: inline-block;
  margin: 0px 20px;
  height: 10px;
  width: 1px;
  border-left: 1px solid #b9b9b9;
}


#container > header > nav > ul li:last-child:after{
  display: none;
}



#container > .content{
  position: absolute;
  top: 50%;
  margin-top: -81px;
  width: 100%;
  background: rgba(0,0,0,0.25);
  overflow : hidden;

  .content-block {
    margin : 30px 0;
  }

}


#container > .content.inactive{
  width: 0px;
}


#container > .content p{
  line-height: 24px;
  text-align: cen;
  margin: 0 0 15px 0;
  padding: 0 0;
}



#container > .content ul{
  margin: 15px 0 0 30px;
  padding: 0;
}


  
#container > .content ul li{
  margin: 0;
  padding: 0 0 0 5px;
  line-height: 24px;
}    


#container > .content ul.icons{
  margin: 0 0;
  padding: 0 0 0 20px;
  width: 288px;
  margin: auto;
}

    
#container > .content ul.icons li{
  display: block;
  float: left;
  margin: 0 32px 0 0;
  padding: 0;
}

  
#container > .content ul.icons li:last-child{
  margin: 0 0 0 0;
}


#container > .content ul.icons li a{
  width: 32px;
  height: 32px;
  font-size: 10px;
  color: rgba(0,0,0,0);
  display: block;
  cursor: pointer;
  -webkit-border-radius: 8px;

  -webkit-transition: all 0.25s ease-out;
  -moz-transition: all 0.25s ease-out;
  -o-transition: all 0.25s ease-out;
  transition: all 0.25s ease-out;
}

      
#container > .content ul.icons li a:hover{
  -webkit-box-shadow:0 0 15px rgba(255,255,255,0.5); 
  -moz-box-shadow: 0 0 15px rgba(255,255,255,0.5);
  box-shadow:0 0 15px rgba(255,255,255,0.5);
}


#container > .content ul.icons li a.twitter{
  background: url("../img/icons/twitter.png");
}


#container > .content ul.icons li a.linkedin{
  background: url("../img/icons/linkedin.png");
}


#container > .content ul.icons li a.github{
  background: url("../img/icons/github.png");
}


#container > .content ul.icons li a.email{
  background: url("../img/icons/email.png");
}


#container > .content ul.icons li a.facebook{
  background: url("../img/icons/facebook.png");
}

  
#container > .content ul.work{
  margin: 0 0;
  padding: 0;
  max-height: 300px;
  width: 800px;
  margin: 0 auto;
}

  
#container > .content ul.work li{
  display: block;
  float: left;
  clear: none;
  margin: 0 0 0;
  padding: 0 0;
  width: 800px;
}


